{
    "dependencies": {
        "Microsoft.NETCore.App": {
            "version": "1.1.0",
            "type": "platform"
        },
        "Dapper": "1.50.2",
        "EFSqlTranslator.Translation": "1.0.0-alpha3",
        "EFSqlTranslator.EFModels": "1.0.0-alpha3",
        "Microsoft.AspNetCore.Owin": "1.1.0",
        "Microsoft.AspNetCore.Routing": "1.1.0",
        "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
        "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
        "Microsoft.AspNetCore.StaticFiles": "1.1.0",
        "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.0",
        "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0",
        "Microsoft.Extensions.Configuration.Json": "1.1.0",
        "Microsoft.Extensions.Logging": "1.1.0",
        "Microsoft.Extensions.Logging.Console": "1.1.0",
        "Microsoft.Extensions.Logging.Debug": "1.1.0",
        "Microsoft.Extensions.Options.ConfigurationExtensions": "1.1.0",
        "Microsoft.CodeAnalysis.CSharp.Scripting": "2.0.0-rc2",
        "Nancy": "2.0.0-clinteastwood ",
        "Nancy.Bootstrappers.Autofac": "2.0.0-clinteastwood ",
        "Nancy.Owin": "2.0.0-clinteastwood ",
        "Nancy.Serialization.JsonNet": "2.0.0-clinteastwood ",
        "Pomelo.EntityFrameworkCore.MySql": "1.1.1-prerelease-10007",
        "Pomelo.EntityFrameworkCore.MySql.Design": "1.1.1-prerelease-10007",
        "Autofac": "4.2.1"
    },

    "tools": {
        "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
    },

    "frameworks": {
        "netcoreapp1.1": {
            "imports": [
                "dotnet5.6",
                "net452"
            ]
        }
    },

    "buildOptions": {
        "emitEntryPoint": true,
        "preserveCompilationContext": true
    },

    "runtimeOptions": {
        "configProperties": {
            "System.GC.Server": true
        }
    },

    "publishOptions": {
        "include": [
            "wwwroot",
            "appsettings.json",
            "web.config"
        ]
    },

    "scripts": {
        "postpublish": [
            "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
        ]
    }
}
